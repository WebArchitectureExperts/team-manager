<p id="notice"><%= notice %></p>

<h1>Organization Teams</h1>

<table>
  <thead>
    <tr>
      <th>Organization</th>
      <th>Team</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @organization_teams.each do |organization_team| %>
      <tr>
        <td><%= organization_team.organization %></td>
        <td><%= organization_team.team %></td>
        <td><%= link_to 'Show', organization_team %></td>
        <td><%= link_to 'Edit', edit_organization_team_path(organization_team) %></td>
        <td><%= link_to 'Destroy', organization_team, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Organization Team', new_organization_team_path %>
