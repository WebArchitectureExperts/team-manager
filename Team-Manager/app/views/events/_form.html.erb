<%= form_for(event) do |f| %>
  <% if event.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(event.errors.count, "error") %> prohibited this event from being saved:</h2>

      <ul>
      <% event.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :event_id %>
    <%= f.number_field :event_id %>
  </div>

  <div class="field">
    <%= f.label :event_type_id %>
    <%= f.text_field :event_type_id %>
  </div>

  <div class="field">
    <%= f.label :event_datetime %>
    <%= f.date_select :event_datetime %>
  </div>

  <div class="field">
    <%= f.label :team_id_id %>
    <%= f.text_field :team_id_id %>
  </div>

  <div class="field">
    <%= f.label :created_by %>
    <%= f.number_field :created_by %>
  </div>

  <div class="field">
    <%= f.label :open_to_public %>
    <%= f.check_box :open_to_public %>
  </div>

  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
