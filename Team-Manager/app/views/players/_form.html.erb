<%= form_for(player) do |f| %>
  <% if player.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(player.errors.count, "error") %> prohibited this player from being saved:</h2>

      <ul>
      <% player.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :player_coach_id_id %>
    <%= f.text_field :player_coach_id_id %>
  </div>

  <div class="field">
    <%= f.label :player_jersey_id %>
    <%= f.number_field :player_jersey_id %>
  </div>

  <div class="field">
    <%= f.label :team_join_date %>
    <%= f.date_select :team_join_date %>
  </div>

  <div class="field">
    <%= f.label :team_left_date %>
    <%= f.date_select :team_left_date %>
  </div>

  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
